# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Site URL (required for auth redirects)
NEXT_PUBLIC_SITE_URL=http://localhost:3000
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id
VERCEL_TOKEN=your-vercel-token

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
# Price IDs from Stripe Dashboard
STRIPE_PRO_MONTHLY_PRICE_ID=price_...
STRIPE_PRO_YEARLY_PRICE_ID=price_...
STRIPE_TEAM_MONTHLY_PRICE_ID=price_...
STRIPE_TEAM_YEARLY_PRICE_ID=price_...

# AI Provider Configuration
# Primary provider: "anthropic" (default) or "openai"
AI_PRIMARY_PROVIDER=anthropic
# Fallback provider: "anthropic", "openai", or "none" (default: auto-selects the other)
AI_FALLBACK_PROVIDER=openai
# Anthropic (Claude) - Primary provider
ANTHROPIC_API_KEY=your-anthropic-key
# OpenAI - Fallback provider
OPENAI_API_KEY=your-openai-key
SENTRY_DSN=your-sentry-dsn
POSTHOG_API_KEY=your-posthog-api-key
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-public-key
POSTHOG_HOST=https://us.posthog.com
