    "@supabase/supabase-js": "^2.87.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.555.0",
    "next": "^16.0.5",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "openai": "^4.59.0",
    "pino": "^10.1.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "pino-pretty": "^13.1.3",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:ci": "next lint --max-warnings=0",
    "typecheck": "tsc --noEmit -p apps/web/tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --runInBand",
    "check": "pnpm lint:ci && pnpm typecheck && pnpm test:ci",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx}\"",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "test:e2e": "playwright test -c apps/web/playwright.config.ts",
    "test:e2e:ui": "playwright test -c apps/web/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test -c apps/web/playwright.config.ts --headed"
  },
  "pnpm-peer-dependency-was-here": "false"
}
