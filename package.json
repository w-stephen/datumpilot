{
  "name": "datumpilot-root",
  "private": true,
  "version": "0.0.0",
  "dependencies": {
    "openai": "^4.59.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:ci": "next lint --max-warnings=0",
    "typecheck": "tsc --noEmit -p apps/web/tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --runInBand",
    "check": "pnpm lint:ci && pnpm typecheck && pnpm test:ci",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx}\""
  },
  "pnpm-peer-dependency-was-here": "false"
}
